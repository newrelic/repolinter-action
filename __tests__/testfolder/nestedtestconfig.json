{
    "$schema": "https://raw.githubusercontent.com/todogroup/repolinter/master/rulesets/schema.json",
    "version": 2,
    "axioms": {},
    "rules": {
        "readme-file-exists": {
            "level": "error",
            "rule": {
                "type": "file-existence",
                "options": {
                    "globsAny": ["README*"],
                    "nocase": true
                }
            }
        }
    }
}